                        <th>Ruang</th>
                        <th>Hari</th>
                        <th>Waktu</th>
                    </tr>
                </thead>
                <% dtd.forEach(function(data) { %>
                <tbody style="text-align:center;">
                    <tr>
                        <td><%=data.mata_kuliah %></td>
                        <td><%=data.kelas %></td>
                        <td><%=data.sks %></td>
                        <td><%=data.ruang %></td>
                        <td><%=data.hari %></td>
                        <td><%=data.waktu %></td>
                    </tr>
                <% }); %>
                </tbody>
            </table>

